<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
</head>
<body>
<% include header.ejs %>
<br>
<!-- Begin page content -->
<div class="container" style="margin-top: 50px">
    <table class="table table-hover">
        <tr>
            <th>#</th>
            <th>위도</th>
            <th>경도</th>
        </tr>

        <% if (data) {
        for (var i = 0; i < data.length; i++) {
            var each = data[i]; %>

        <tr>
            <td><%= i + 1 %></td>
            <td><%= each.latitude %></td>
            <td><%= each.longitude %></td>
        </tr>

        <% }
        } %>

    </table>
</div>

</body>
</html>